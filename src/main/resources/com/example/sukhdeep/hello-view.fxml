<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="666.0" prefWidth="682.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.sukhdeep.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
    <Button onAction="#onHelloButtonClick" text="Bringing the Data" />
    <Button onAction="#insert" mnemonicParsing="false" text="insert" />
    <Button onAction="#update" mnemonicParsing="false" text="Update" />
    <Button onAction="#delete" mnemonicParsing="false" text="Delete" />
    <Button onAction="#viewdata" mnemonicParsing="false" text="view data" />

    <TextField fx:id="iClientName"/>
    <TextField fx:id="iage" />
    <TextField fx:id="iaddress"/>
    <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0">
        <columns>
            <TableColumn fx:id="id" prefWidth="75.0" text="id" />
            <TableColumn fx:id="ClientName" prefWidth="75.0" text="ClientName" />
            <TableColumn fx:id="age" prefWidth="75.0" text="age" />
            <TableColumn fx:id="address" prefWidth="75.0" text="address" />
        </columns>
    </TableView>
</VBox>